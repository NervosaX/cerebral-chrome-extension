{
    "name": "Cerebral Debugger",
    "version": "0.14",
    "description": "A debugger for web app state control and insight",
    "devtools_page": "devtools.html",
    "content_scripts": [
        {
            "all_frames": false,
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
}
